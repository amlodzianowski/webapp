upstream app {
    server app.adrian-training.aws.logicworks.net:18000;
}

upstream local {
    server localhost:16000;
}
server {
        listen 80;

        location / {
            root   /data;
            index  index.html;
        }

        location /youtube {
                uwsgi_pass app;
        }

        location /info {
                uwsgi_pass local;
        }
}
